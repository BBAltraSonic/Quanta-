# Quanta Environment Configuration Template
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Supabase Configuration (get these from your Supabase project dashboard)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key-here

# AI Service Configuration (optional - can be empty for local testing)
OPENROUTER_API_KEY=your-openrouter-api-key-here
HUGGINGFACE_API_KEY=your-huggingface-api-key-here

# Sentry Configuration (optional - can be empty for local testing)
SENTRY_DSN=your-sentry-dsn-here

# Environment Configuration
ENVIRONMENT=development
DEBUG_MODE=true
ENABLE_LOGGING=true

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true